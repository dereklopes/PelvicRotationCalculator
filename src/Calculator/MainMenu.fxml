<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<VBox minHeight="400.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Calculator.MainController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox spacing="2.0">
               <children>
                     <Label text="Focal film distance:" />
                  <HBox spacing="5.0">
                     <children>
                        <TextField id="focalFilmDistanceField" fx:id="focalFilmDistanceField" prefWidth="100.0" />
                        <ChoiceBox prefWidth="50.0" />
                     </children>
                  </HBox>
                  <Separator layoutX="10.0" layoutY="148.0" prefWidth="200.0">
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </Separator>
                     <Label text="S2/MFH Distance to Film (lateral):" />
                  <HBox spacing="5.0">
                     <children>
                        <TextField id="s2ToFilmLateralField" fx:id="s2ToFilmLateralField" prefWidth="100.0" />
                        <ChoiceBox prefWidth="50.0" />
                     </children>
                  </HBox>
                     <Label text="S2 to MFH (lateral, from film):" />
                  <HBox spacing="5.0">
                     <children>
                        <TextField id="s2ToMFHLateralFilmField" fx:id="s2ToMFHLateralFilmField" prefWidth="100.0" />
                        <ChoiceBox prefWidth="50.0" />
                     </children>
                  </HBox>
                  <Separator layoutX="10.0" layoutY="194.0" prefWidth="200.0">
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </Separator>
                     <Label text="S2 Distance to Film (A-P):" />
                  <HBox spacing="5.0">
                     <children>
                        <TextField id="s2ToFilmAPField" fx:id="s2ToFilmAPField" prefWidth="100.0" />
                        <ChoiceBox prefWidth="50.0" />
                     </children>
                  </HBox>
                  <Label layoutX="10.0" layoutY="163.0" text="S2 to MFH (A-P, from film):" />
                  <HBox spacing="5.0">
                     <children>
                        <TextField id="s2ToMFHAPFilmField" fx:id="s2ToMFHAPFilmField" layoutX="10.0" layoutY="182.0" prefWidth="100.0" />
                        <ChoiceBox prefWidth="50.0" />
                     </children>
                  </HBox>
                  <Separator prefWidth="200.0">
                     <padding>
                        <Insets top="10.0" />
                     </padding></Separator>
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <Label text="Result Units:" />
                        <ChoiceBox prefWidth="75.0" />
                     </children>
                  </HBox>
                  <VBox alignment="CENTER">
                     <children>
                           <Button id="calculateButton" fx:id="calculateButton" defaultButton="true" mnemonicParsing="false" onAction="#calculateButtonPressed" text="Calculate" />
                     </children>
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                  </VBox>
               </children>
            </VBox>
            <TableView fx:id="resultsTable" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="focalFilmDistanceCol" minWidth="-Infinity" prefWidth="100.0" text="Focal film dist." />
                <TableColumn fx:id="s2ToFilmLateralCol" minWidth="-Infinity" prefWidth="100.0" text="S2 to Film (lat.)" />
                <TableColumn fx:id="s2ToMFHLateralFilmCol" minWidth="-Infinity" prefWidth="125.0" text="S2 to MFH (lat., film)" />
                <TableColumn fx:id="s2ToMFHTrueCol" minWidth="-Infinity" prefWidth="100.0" text="True S2 to MFH" />
                <TableColumn fx:id="s2ToFilmAPCol" minWidth="-Infinity" prefWidth="100.0" text="S2 to Film (A-P)" />
                <TableColumn fx:id="MFHToFilmTrueCol" minWidth="-Infinity" prefWidth="105.0" text="True MFH to Film" />
                <TableColumn fx:id="s2ToMFHAPFilmCol" minWidth="-Infinity" prefWidth="125.0" text="S2 to MFH (A-P, film)" />
                <TableColumn fx:id="s2ToMFHOffsetCol" minWidth="-Infinity" prefWidth="135.0" text="True S2 to MFH Offset" />
                <TableColumn fx:id="rotationalDegreeCol" minWidth="-Infinity" prefWidth="110.0" text="Rotational Degree" />
                <TableColumn minWidth="-Infinity" prefWidth="110.0" text="STEP 14, FINAL" />
              </columns>
            </TableView>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</VBox>
